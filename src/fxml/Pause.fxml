<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="600.0" prefWidth="500.0" stylesheets="@../css/Pause.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AllClasses.Pause">
   <children>
      <ImageView fitHeight="600.0" fitWidth="500.0">
         <image>
            <Image url="@../img/pause.png" />
         </image>
      </ImageView>
      <Button fx:id="Continue" layoutX="175.0" layoutY="150.0" mnemonicParsing="false" onAction="#pushContinue" prefHeight="50.0" prefWidth="150.0" text="Continue" />
      <Button fx:id="Menu" layoutX="175.0" layoutY="250.0" mnemonicParsing="false" onAction="#pushMenu" prefHeight="50.0" prefWidth="150.0" text="Menu" />
      <Button fx:id="Exit" layoutX="175.0" layoutY="350.0" mnemonicParsing="false" onAction="#pushExit" prefHeight="50.0" prefWidth="150.0" text="Exit" />
   </children>
</AnchorPane>
